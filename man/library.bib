@Article{	  gilman2010,
  abstract	= {Predicting the impacts of climate change on species is one
		  of the biggest challenges that ecologists face. Predictions
		  routinely focus on the direct effects of climate change on
		  individual species, yet interactions between species can
		  strongly influence how climate change affects organisms at
		  every scale by altering their individual fitness,
		  geographic ranges and the structure and dynamics of their
		  community. Failure to incorporate these interactions limits
		  the ability to predict responses of species to climate
		  change. We propose a framework based on ideas from
		  global-change biology, community ecology, and invasion
		  biology that uses community modules to assess how species
		  interactions shape responses to climate change.},
  author	= {Gilman, Sarah E and Urban, Mark C and Tewksbury, Joshua
		  and Gilchrist, George W and Holt, Robert D},
