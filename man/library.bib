  keywords	= {Animals,Biological,Biological Evolution,Food
		  Chain,Lupinus,Models,Moths,Population Dynamics,Predatory
		  Behavior},
  month		= {apr},
  number	= {1661},
  pages		= {1435--1442},
  pmid		= {19324814},
  title		= {{Trophic interactions and range limits: the diverse roles
		  of predation.}},
  url		= {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2677217{\&}tool=pmcentrez{\&}rendertype=abstract},
  volume	= {276},
