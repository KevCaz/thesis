@Article{	  chamberlain2014,
  abstract	= {The net effects of interspecific species interactions on
		  individuals and populations vary in both sign (-, 0, +) and
		  magnitude (strong to weak). Interaction outcomes are
		  context-dependent when the sign and/or magnitude change as
		  a function of the biotic or abiotic context. While context
		  dependency appears to be common, its distribution in nature
		  is poorly described. Here, we used meta-analysis to
		  quantify variation in species interaction outcomes
		  (competition, mutualism, or predation) for 247 published
		  articles. Contrary to our expectations, variation in the
		  magnitude of effect sizes did not differ among species
		  interactions, and while mutualism was most likely to change
		  sign across contexts (and predation least likely),
		  mutualism did not strongly differ from competition. Both
		  the magnitude and sign of species interactions varied the
		  most along spatial and abiotic gradients, and least as a
		  function of the presence/absence of a third species.
		  However, the degree of context dependency across these
		  context types was not consistent among mutualism,
		  competition and predation studies. Surprisingly, study
		  location and ecosystem type varied in the degree of context
		  dependency, with laboratory studies showing the highest
		  variation in outcomes. We urge that studying context
		  dependency per se, rather than focusing only on mean
		  outcomes, can provide a general method for describing
		  patterns of variation in nature.},
  author	= {Chamberlain, Scott A and Bronstein, Judith L and Rudgers,
		  Jennifer A},
  doi		= {10.1111/ele.12279},
  editor	= {Etienne, Rampal},
  file		= {:Users/KevCaz/Dropbox/Papers/Ecology Letters/Chamberlain,
		  Bronstein, Rudgers{\_}2014{\_}How context dependent are
		  species interactions.pdf:pdf},
  issn		= {1461023X},
  journal	= {Ecology Letters},
  keywords	= {2014,coefficient of variation,community
		  context,conditionality,distributed outcomes,ecology
		  letters,interaction,meta-analysis,strength},
  month		= {jul},
  number	= {7},
  pages		= {881--890},
  pmid		= {24735225},
  title		= {{How context dependent are species interactions?}},
  url		= {http://www.ncbi.nlm.nih.gov/pubmed/24735225
		  http://doi.wiley.com/10.1111/ele.12279},
  volume	= {17},
  year		= {2014}
}

